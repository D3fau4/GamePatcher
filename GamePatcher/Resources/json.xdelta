    "scr_text_slash_scr_text_gml_98_0": "* (¿Quieres cogerlo?)/ ",
    "scr_text_slash_scr_text_gml_91_0": "#Sí",
    "scr_text_slash_scr_text_gml_92_0": "#No",
    "scr_text_slash_scr_text_gml_111_0": "* (Obtienes la Esquirla.)/%",
    "scr_text_slash_scr_text_gml_118_0": "* (Pues ahí se queda.)/%",
    "scr_text_slash_scr_text_gml_103_0": "* (No se ve nada.)/%",
    "obj_savepoint_slash_Other_10_gml_26_0": "* Dejas que la luz rodee tu cuerpo./",
